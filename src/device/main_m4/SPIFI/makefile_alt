	-@echo 'Performing post-build steps'
	-arm-none-eabi-size "main_m4.axf" ; arm-none-eabi-objcopy -v -O binary "main_m4.axf" "main_m4.bin" ; checksum -p LPC4330 -d "main_m4.bin" ; arm-none-eabi-objcopy -I binary -O ihex main_m4.bin pixy_firmware.hex
	-@echo ' '

